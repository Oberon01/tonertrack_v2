# TonerTrack example environment variables
# Copy to .env and set values locally (do NOT commit .env)

# Optional: override default data directory (absolute path)
TONERTRACK_DATA_DIR=

# Comma-separated list of Windows print servers to sync from (optional)
TONERTRACK_PRINT_SERVERS=

# Mapping of print-server to view (format: "\\\\server01:Label01,\\\\:server02:Label02") (optional)
TONERTRACK_PRINT_SERVER_VIEWS=

# Disable opening browser on start
TONERTRACK_NO_BROWSER=0
